<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>com.samarskii.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <!--http://localhost:8080/registrationwebapp_db_war/-->
    <servlet-name>registration</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>info</servlet-name>
    <servlet-class>com.samarskii.InfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <!--localhost:8080/registrationwebapp_db_war/info?login=admin&password=admin-->
    <servlet-name>info</servlet-name>
    <url-pattern>/info</url-pattern>
  </servlet-mapping>
</web-app>
